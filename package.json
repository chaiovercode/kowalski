{
  "name": "kowalski-analytics",
  "version": "1.0.0",
  "description": "Kowalski Analytics - Data analysis skill for Claude Code",
  "author": "Vivek",
  "license": "MIT",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "bin": {
    "kowalski": "src/cli.ts",
    "kowalski-canvas": "src/cli.ts"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./analytics": {
      "import": "./src/canvases/analytics/index.ts",
      "types": "./src/canvases/analytics/index.ts"
    },
    "./insights": {
      "import": "./src/canvases/analytics/insights.ts",
      "types": "./src/canvases/analytics/insights.ts"
    },
    "./understanding": {
      "import": "./src/canvases/analytics/understanding.ts",
      "types": "./src/canvases/analytics/understanding.ts"
    },
    "./hypotheses": {
      "import": "./src/canvases/analytics/hypotheses.ts",
      "types": "./src/canvases/analytics/hypotheses.ts"
    },
    "./api": {
      "import": "./src/api/index.ts",
      "types": "./src/api/index.ts"
    }
  },
  "files": [
    "src",
    "commands",
    "sample_data"
  ],
  "scripts": {
    "start": "bun run src/cli.ts",
    "show": "bun run src/cli.ts show",
    "spawn": "bun run src/cli.ts spawn",
    "test": "bun test",
    "postinstall": "node scripts/setup-command.js || true"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "ink": "^6.6.0",
    "ink-spinner": "^5.0.0",
    "react": "^19.2.3",
    "react-devtools-core": "^7.0.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.7"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "keywords": [
    "claude",
    "claude-code",
    "analytics",
    "data-analysis",
    "kowalski",
    "eda",
    "visualization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vivek/kowalski"
  }
}
